@model Golf_6.Models.Medlem

@{
    ViewBag.Title = "Personuppgifter";
    Layout = "~/Views/Shared/_LayoutMedlem.cshtml";
}

<div class="container">
    <div class="row">
        <h2>Personuppgifter</h2>

        <form class="form-horizontal">
            <fieldset>
          
                <legend>Uppdatera dina&nbsp;personuppgifter&nbsp;här:</legend>
            
                <div class="form-group">
                   
                    <div class="col-md-4">
                        <input id="textinput" name="textinput" placeholder="Förnamn" class="form-control input-md" required="" type="text">
                        <span class="help-block"> </span>
                    </div>
                </div>
               
                <div class="form-group">
                   
                    <div class="col-md-4">
                        <input id="textinput" name="textinput" placeholder="Efternamn" class="form-control input-md" required="" type="text">
                        <span class="help-block"> </span>
                    </div>
                </div>
               
                <div class="form-group">
               
                    <div class="col-md-4">
                        <input id="textinput" name="textinput" placeholder="Gatuadress" class="form-control input-md" required="" type="text">
                        <span class="help-block"> </span>
                    </div>
                </div>
               
                <div class="form-group">
                   
                    <div class="col-md-4">
                        <input id="textinput" name="textinput" placeholder="Postnummer" class="form-control input-md" required="" type="text">
                        <span class="help-block"> </span>
                    </div>
                </div>
         
                <div class="form-group">
                    <div class="col-md-4">
                        <input id="textinput" name="textinput" placeholder="Ort" class="form-control input-md" required="" type="text">
                        <span class="help-block"> </span>
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-md-4">
                        <input id="textinput" name="textinput" placeholder="Email" class="form-control input-md" required="" type="text">
                        <span class="help-block"> </span>
                    </div>
                </div>
                <!-- Text input-->
                <div class="form-group">
                    <div class="col-md-4">
                        <input id="textinput" name="textinput" placeholder="Telefonnummer" class="form-control input-md" required="" type="text">
                        <span class="help-block"> </span>
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-md-4">
                        <input id="textinput" name="textinput" placeholder="HCP" class="form-control input-md" required="" type="text">
                        <span class="help-block"> </span>
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-md-4">
                        <button class="btn btn-primary">Uppdatera</button>
                    </div>
                </div>
            </fieldset>
        </form>

    </div>
</div>

