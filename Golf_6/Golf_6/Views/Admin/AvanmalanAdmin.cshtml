
@model IList<Golf_6.Models.Admin>
@using Golf_6.Controllers;
@using Golf_6.Models;
@using System.Collections.Generic;
@using System.Data;

@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}


@using (Html.BeginForm("AvanmalanAdmin")) { 


<div>
        @foreach
                (var admin in
                Model)
        {<ul class="list-group">
            <li class="list-group-item">@admin.GolfID</li>
            <li class="list-group-item">Förnamn:- @admin.Fornamn</li>
            <li class="list-group-item">Efternamn:- @admin.Efternamn</li>
          
        </ul>
        }
</div>
}

